# This program removes the duplicates in a list
numbers = [1, 3, 1, 5, 6, 4, 3, 9, 0, 3, 7, 8]
numbers_without_duplicates = numbers.copy()

for number in numbers_without_duplicates:
    if numbers_without_duplicates.count(number) > 1:
        numbers_without_duplicates.remove(number)
print(numbers_without_duplicates)
